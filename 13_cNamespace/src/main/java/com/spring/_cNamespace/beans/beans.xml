<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!--C-NameSpace -->
	<bean id="patient" class="com.spring._cNamespace.Patient" c:id="1234"
		c:name="Ruhul" c:age="30" c:address-ref="address">
	</bean>
	<bean id="address" class="com.spring._cNamespace.Address" c:_0="Panthapath"
		c:_1="Dhaka">
		<!-- c:street="Panthapath" c:city="Dhaka" -->
	</bean>

</beans>
